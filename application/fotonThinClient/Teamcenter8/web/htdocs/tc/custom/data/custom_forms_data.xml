<xml id="reportDemo">
<form name="Report Demo" style="display:none">

</form>
FormForm += '<customforms>' + NL
FormForm += '<custom form_type="reportDemo22">' + NL
FormForm += '<field name="name" length="' + imanText( 'web_wso_name_size' ) + '" required="true" displayname="' + imanText( 'web_name_field' ) + '"></field>' + NL
FormForm += '<field name="description" type="Note" length="' + imanText( 'web_wso_desc_size' ) + '" displayname="' + imanText( 'web_desc_field' ) + '"></field>' + NL
FormForm += '<field name="webitk_code" displayname="Code" value="101"></field>' + NL
FormForm += '</custom>' + NL
FormForm += '</customforms>' + NL
